{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/login",
      "dest": "/api/auth/login"
    },
    {
      "src": "/api/auth/register",
      "dest": "/api/auth/register"
    },
    {
      "src": "/api/campaigns",
      "dest": "/api/campaigns"
    },
    {
      "src": "/api/scenes",
      "dest": "/api/scenes"
    }
  ]
}